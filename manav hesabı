import java.util.Scanner;
public class Main {
    public static void main(String[] args) {

        // Değişkenleri tanımla
        int Armutkg, Elmakg, Domateskg, Muzkg, Patlıcankg ;
        double birimmKilogram, Toplamfiyat, Armut=2.14, Elma=3.67, Domates=1.11, Muz=0.95, Patlıcan=5.00;

        Scanner input = new Scanner(System.in);

        System.out.print("Armut Kaç Kilo ? : ");
        Armutkg = input.nextInt();

        System.out.print("Elma Kaç Kilo ? : ");
        Elmakg = input.nextInt();

        System.out.print("Domates Kaç Kilo ? : ");
        Domateskg = input.nextInt();

        System.out.print("Muz Kaç Kilo ? : ");
        Muzkg = input.nextInt();

        System.out.print("Patlıcan Kaç Kilo ? : ");
        Patlıcankg = input.nextInt();

        Toplamfiyat = ((Armut*Armutkg) + (Elma*Elmakg) + (Domates*Domateskg) + (Muz*Muzkg) + (Patlıcan*Patlıcankg));
        System.out.print("Toplam Ödenecek Tutar : " + Toplamfiyat + " TL ");





    }
}
